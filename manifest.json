{
    "manifest_version": 3,
    "name": "New Tab Page Override",
    "version": "1.0",
    "description": "Redirect new tab to a specific page (in this case ecosia.org)",
    "action": {},
    "permissions": ["tabs", "activeTab"],
    "host_permissions": ["https://*/*"],
    "background": {
        "service_worker": "background.js"
    }   
}
